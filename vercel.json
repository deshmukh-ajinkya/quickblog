{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "production": true
      }
    }
  ],
  "github": {
    "enabled": true,
    "autoJobCancelation": true,
    "silent": false,
    "deployments": {
      "production": {
        "branches": [
          "main"
        ],
        "autoDeploy": true
      }
    },
    "ignoreBuilds": [
      "*/**"
    ],
    "include": [
      "main"
    ]
  },
  "git": {
    "deploymentEnabled": true
  }
}