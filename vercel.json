{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "github": {
    "enabled": true,
    "autoJobCancelation": true,
    "silent": false,
    "deployments": {
      "production": {
        "branches": ["main"]
      },
      "preview": {
        "branches": []
      }
    }
  },
  "git": {
    "deploymentEnabled": true
  }
}
